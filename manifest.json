{
   "background": {
      "scripts": [
         "libs/lodash.js",
         "scripts/background.js"
      ]
   },
   "browser_action": {
      "default_icon": "slLogo.png",
      "default_popup": "popup.html",
      "default_title": "Add to List!"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval' https://ajax.googleapis.com https://maxcdn.bootstrapcdn.com; object-src 'self'",
   "description": "Click the Add Page button to save your favorite item links to access later!",
   "icons": {
      "16": "slLogo.png",
      "48": "slLogo.png",
      "128": "slLogo.png"
   },
   "manifest_version": 2,
   "name": "Swift-Shopping!",
   "permissions": [
      "activeTab",
      "\u003Call_urls>"
   ],
   "version": "1.0.3"
}